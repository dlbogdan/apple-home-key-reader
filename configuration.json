{
    "logging": {
        "level": 20
    },

    "nfc": {
        "port": "ttyS0", 
        "driver": "pn532",
        "broadcast": true
    },

    "hap": {
        "port": 51926,
        "persist": "hap.state",
        "default": "locked"
    },

    "homekey": {
        "persist": "homekey.json",
        "express": true,
        "finish": "silver",
        "flow": "fast"
    },

    "lock": {
        "socketfile": "/dev/shm/physlock-ipc.socket"
    }
}
