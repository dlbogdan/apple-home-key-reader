{
    "logging": {
        "level": 20
    },

    "nfc": {
        "port": "ttyS0", 
        "driver": "pn532",
        "broadcast": true
    },

    "hap": {
        "port": 51926,
        "persist": "hap.state",
        "default": "locked",
        "display_name": "eQ-3 Lock"
    },

    "homekey": {
        "persist": "homekey.json",
        "express": true,
        "finish": "silver",
        "flow": "fast"
    },

    "physical_lock_ipc": {
        "socketfile": "/dev/shm/physlock-ipc.socket"
    },

    "mqtt_client": {
        "server_ip": "127.0.0.1"
    }
}
